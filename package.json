{
  "name": "elm-ui",
  "version": "0.0.1",
  "description": "An example package for an Elm UI using Elm 0.17",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "css-loader": "^0.23.1",
    "elm": "^0.17.0",
    "elm-hot-loader": "^0.2.2",
    "elm-webpack-loader": "^3.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "gulp": "^3.9.1",
    "html-webpack-plugin": "^2.16.0",
    "image-webpack-loader": "^1.8.0",
    "no-export-loader": "^1.0.0",
    "node-sass": "^3.6.0",
    "postcss-loader": "^0.9.1",
    "required-loader": "^1.0.0",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.12.0"
  },
  "private": true,
  "scripts": {
    "clean": "npm run clean:dist && npm run clean:elm",
    "clean:dist": "rimraf dist",
    "clean:elm-full": "rimraf elm-stuff/build-artifacts",
    "clean:elm": "rimraf elm-stuff/build-artifacts/*/user",
    "clobber:elm": "rimraf elm-stuff",
    "start": "webpack-dev-server --hot --inline",
    "build": "npm run clean && webpack",
    "watch": "webpack watch"
  },
